{
  "name": "setup-talenta",
  "version": "0.0.0",
  "private": true,
  "description": "Setup talenta action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint . --ext .ts",
    "prepackage": "tsc",
    "package": "ncc build --license licenses.txt",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/github": "^5.0.0",
    "@actions/tool-cache": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^16.4.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vercel/ncc": "^0.28.6",
    "codecov": "^3.8.2",
    "eslint": "^7.29.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.2"
  }
}
